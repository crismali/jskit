<tr id="prop-{{name}}" class="property item{{#if access}} {{access}}{{/if}}{{#if deprecated}} deprecated{{/if}}{{#if extended_from}} inherited{{/if}}">
    <td class="prop-name">
        {{name}}
        {{#if deprecated}}
            <span class="flag deprecated"{{#if deprecationMessage}} title="{{deprecationMessage}}"{{/if}}>deprecated</span>
        {{/if}}
        {{#if access}}
            <span class="flag {{access}}">{{access}}</span>
        {{/if}}
        {{#if final}}
            <span class="flag final">const</span>
        {{/if}}
        {{#if static}}
            <span class="flag static">static</span>
        {{/if}}
    </td>
    <td class="prop-type">{{#crossLink type}}{{/crossLink}}</td>
    <td class="prop-description">{{{propertyDescription}}}</td>
</tr>
